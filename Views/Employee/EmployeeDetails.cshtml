    </tr>
  </thead>
  <tbody>
   @foreach (MVC_Demo.Models.Employee item in (IEnumerable<Employee>) ViewData["Empoyees"])
		{
			
			<tr>
				<td>
					@item.empId
				</td>
				<td>
					@item.empName
				</td>
				<td>
					@item.empSalary
				</td>
			</tr>

		}
    
  </tbody>
</table>