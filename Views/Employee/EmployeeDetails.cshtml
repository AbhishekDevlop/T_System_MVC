<h2>This is EmployeeDetails</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Salary</th>
    </tr>
  </thead>
  <tbody>
   @foreach (MVC_Demo.Models.Employee item in ViewBag.Employees)
		{
			
			<tr>
				<td>
					@item.empId
				</td>
				<td>
					@item.empName
				</td>
				<td>
					@item.empSalary
				</td>
			</tr>

		}
    
  </tbody>
</table>
<table class="table">
  <thead>
